          for(var j = 0; j < states.length; j++){
            var statesKey = country[i].states[j]
             var name = statesKey.state_name;
             var code = statesKey.state_code;
             var option = `<option value=${code}>${name}</options>`;
             $('#states').append(option);
          }
          break;
        }
      }
    }
  });

  
  function getCountry(){
    var states = []
    for(var i = 0; i < country.length; i++){
      var c = country[i]
      var value = c.country_code;
      var title = c.country_name;
      states = c.states;

      if(states == undefined){
        var stateOption = `<option>No State</options>`;
        $('#states').append(stateOption);
      }
      var option = `<option value=${value}>${title}</options>`;
      $('#country').append(option);
    };
  }
});




































































</script>
